<% layout("/layout/boilerplate") %>

<form action="/listing" method="post">
    <label for="title">Title</label>
    <input type="text" name="listing[title]" id="title" placeholder="Enter the title">
    <br>
    <label for="description">Description</label>
    <input type="text" name="listing[description]" id="description" placeholder="write something about place">
    <br>
    <label for="location">Location</label>
    <input type="text" name="listing[location]" id="location" placeholder="enter the location">
    <br>
    <label for="price">Price</label>
    <input type="number" name="listing[price]" id="price" placeholder="price enter here">
    <br>
    <label for="image">Image</label>
    <input type="text" name="listing[image]" id="image" placeholder="enter your Image url">
    <br>
    <label for="conutry">Conutry</label>
    <input type="text" name ="listing[conutry]" id="conutry" placeholder="enter your country name ">
    <br>
    <button type="submit">submit</button>
</form>